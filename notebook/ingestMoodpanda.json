				"source": [
					"import requests\r\n",
					"from azure.keyvault.secrets import SecretClient\r\n",
					"from azure.identity import DefaultAzureCredential"
				],
				"execution_count": 7
			},
			{
				"cell_type": "code",
				"source": [
					"KEY_VAULT_NAME = 'quant-self-dev-kv'\r\n",
					"SECRET_NAME_MOODPANDA_PASSWORD = 'moodpanda-password'\r\n",
					"SECRET_NAME_MOODPANDA_USERNAME = 'moodpanda-username'"
				],
				"execution_count": 8
			},
			{
				"cell_type": "code",
				"metadata": {
					"jupyter": {
						"source_hidden": false,
						"outputs_hidden": false
					},
					"nteract": {
						"transient": {
							"deleting": false
						}
					}
				},
				"source": [
					"TokenLibrary.getSecret(KEY_VAULT_NAME, SECRET_NAME_MOODPANDA_PASSWORD, \"quantselfkv\")\r\n",
					""
				],
				"execution_count": 16
			},
			{
				"cell_type": "code",
				"metadata": {
					"jupyter": {
						"source_hidden": false,
						"outputs_hidden": false
					},
					"nteract": {
						"transient": {
							"deleting": false
						}
					}
				},
				"source": [
					"credential = DefaultAzureCredential()\r\n",
					"client = SecretClient(vault_url=f\"https://{KEY_VAULT_NAME}.vault.azure.net/\", credential=credential)\r\n",
					"moodpanda_password = client.get_secret(SECRET_NAME_MOODPANDA_PASSWORD).value\r\n",
					"moodpanda_username = client.get_secret(SECRET_NAME_MOODPANDA_USERNAME).value\r\n",
					""
				],
				"execution_count": 12
			},
			{
				"cell_type": "code",
				"metadata": {
					"jupyter": {
						"source_hidden": false,
						"outputs_hidden": false
					},
					"nteract": {
						"transient": {
							"deleting": false
						}
					}
				},
				"source": [
					"auth_data = {\"username\": moodpanda_username, \r\n",
					"             \"password\": moodpanda_password} \r\n",
					"headers = {\"Content-Type\": \"application/json\"}\r\n",
					"AUTH_URL = \"https://moodpanda.com/api/users/authenticate\"\r\n",
					"auth_resp = requests.post(url=AUTH_URL,\r\n",
					"                          json=auth_data,\r\n",
					"                          headers=headers,\r\n",
					"                          timeout=5)\r\n",
					"jwt_token = auth_resp.json()[\"jwtToken\"]"
				],
				"execution_count": 11
			},
			{
				"cell_type": "code",
				"metadata": {
					"jupyter": {
						"source_hidden": false,
